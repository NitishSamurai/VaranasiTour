Drupal.behaviors.commonUtilityAnchor={attach:function(context){jQuery("a").each(function(){var images=jQuery(this).find("img");if(images.length){jQuery(images).each(function(){var $parent=jQuery(this).parent();if((jQuery(this).attr("alt")==null||jQuery(this).attr("alt")=="")&&$parent.attr("title")!=""){jQuery(this).attr("alt",$parent.attr("title"));if(jQuery(this).attr("title")==$parent.attr("title")){jQuery(this).attr("title","");}}});}else{if((jQuery(this).attr("title")==null||jQuery(this).attr("title")=="")){var anchorvalue=jQuery(this).text().replace(/\s+/g," ");if(jQuery(this).hasClass("ext")){anchorvalue+=" (External Site that opens in a new window)";jQuery(this).attr("title",anchorvalue);jQuery(this).attr("target","_blank");}else{jQuery(this).attr("title",anchorvalue);}}}});jQuery("input").each(function(){if((jQuery(this).attr("title")==null)||(jQuery(this).attr("title")=="")){var anchorvalue=jQuery(this).val();jQuery(this).attr("title",anchorvalue);}});}};